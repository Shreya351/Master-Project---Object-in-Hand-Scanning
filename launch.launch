<!-- Open an Ensenso stereo file camera in a node. -->

<launch>
  <arg name="serial" doc="Serial of the stereo file camera to be created" default="stereo_cam" />
  <arg name="path" doc="The path to the stereo file camera directory or zip file" />
  <arg name="link_frame" doc="The camera's link tf frame (default: camera_frame)" default="" />
  <arg name="target_frame" doc="The tf frame the data will be returned in (default: camera_frame)" default="" />

  <node pkg="ensenso_camera" type="ensenso_camera_node" name="ensenso_camera_node">
    <param name="serial" type="string" value="Mikado"/>
    <param name="file_camera_path" type="string" value="/home/optonic-shared-pc/Documents/Work/Test/FileCamera"/>
    <param name="link_frame" type="string" value="$(arg link_frame)" />
    <param name="target_frame" type="string" value="Workspace" />
  </node>
  <node pkg="node1" type="mikado_shreya.py" name="milado_shreya" output="screen" />
  <node type="rviz" name="rviz" pkg="rviz" args="-d `rospack find package_name`/rviz/config_file.rviz" />
</launch>
